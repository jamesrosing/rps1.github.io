		<script type="text/javascript">

			//<![CDATA[
				$(document).ready(function() {
					// updated 07 Oct
					// PRODUCT BUILD RIGHT COL STICKY
						//var sticky_navigation_offset_top = $('.twoColumnsRight').offset().top;
						// updated 28 Feb
						if($('.twoColumnsRight').length) {
							var sticky_navigation = function(){
								var right_prod = $('.rightProduct').height();
								var sticky_navigation_offset_top = $('.twoColumnsRight').offset().top;
								var right_col = $(".twoColumnsRight").height();
								var win_height = $(window).height();
								var scroll_top = $(window).scrollTop();


							// our current vertical position from the top
								// if we've scrolled more than the navigation, change its position to fixed to stick to top, otherwise change it back to relative
								//alert(scroll_top - sticky_navigation_offset_top);
								if (scroll_top > sticky_navigation_offset_top && $( window ).width() > 680 && win_height > right_prod ) {
									var toppos = scroll_top - sticky_navigation_offset_top;
									if(toppos > (right_col - right_prod)) {
										$('.rightProduct').css({ 'position': 'absolute', 'bottom':0, 'top':'auto', 'right': 0 });
									} else {
										$('.rightProduct').css({ 'position': 'absolute', 'top':toppos, 'bottom':'auto', 'right': 0 });
									}

								} else {
									$('.rightProduct').css({ 'position': 'relative', 'top':0 });
								}
							};

							// run our function on load
							sticky_navigation();

							// and run it again every time you scroll
							$(window).scroll(function() {
								sticky_navigation();
							});
							$(window).resize(function() {
								sticky_navigation();
							});

							$(".twoColumnsRight").resize(function() {
								sticky_navigation();
							});
							$(".rightProduct").resize(function() {
								sticky_navigation();
							});
						}

							$('.libertyPagination div').click(function(){
								if($(this).find('span').css("display") == 'none') {
									jwplayer("myElement").pause(true);
								} else {
									//jwplayer("myElement").play();
								}
							});
						// /updated 28 Feb
					// /updated 07 Oct
					// /PRODUCT BUILD RIGHT COL STICKY
				});
			//]]>

		</script>

	

	

	

		

	    <script type="text/javascript">

			var ErrorHandler = function(statusCode, statusMsg)
			{
			alert('Status: ' + statusCode + ', ' + statusMsg);

			}

			var submitRefCode= function() {

			var e = new configObj();
			e.setCallbackHandler(returnRefCode);
			e.setErrorHandler(ErrorHandler);
			e.setForm('refCodeForm');
			e.setHTTPMethod('POST');
			e.setRefCode();

			}


			var returnRefCode = function(results) {

			if (results.ERROR ==1) {

			document.getElementById('refCodeMsg').innerHTML = results.MESSAGE;

			} else if(results.ERROR ==2) {
			document.getElementById('codeNameDiv').innerHTML = results.CODENAME;
			document.getElementById('empRow').style.display='';
			document.getElementById('refCodeMsg').innerHTML = results.MESSAGE;

			} else {
			document.getElementById('divRefCode').innerHTML =results.REFCODE;
			popupOpen("referencePopup");
			}
			}



			var submitRefCode2017= function() {

			var e = new configObj();
			e.setCallbackHandler(returnRefCode2017);
			e.setErrorHandler(ErrorHandler);
			e.setForm('refCodeForm');
			e.setHTTPMethod('POST');
			e.setRefCode();

			}

			
			var returnRefCode2017 = function(results) {

			if (results.ERROR ==1) {

			document.getElementById('refCodeMsg').innerHTML = results.MESSAGE;

			} else if(results.ERROR ==2) {
			document.getElementById('empRow').style.display='';
			document.getElementById('codeNameDiv').innerHTML = results.CODENAME;
			document.getElementById('refCodeMsg').innerHTML = results.MESSAGE;

			} else {
			document.getElementById('refCodeSection').innerHTML = '<div class="refcode-recieved" style="display:block"><span><label>Ref Code</label><h3>' + results.REFCODE + '</h3></span><span> <a href="/index.cfm?&refcodelogoff=1" class="ref-exit">remove</i></a></span></div>';

			}
			}
			


			var submitRefCodeLogin= function() {

			var e = new configObj();
			e.setCallbackHandler(returnRefCodeLogin);
			e.setErrorHandler(ErrorHandler);
			e.setForm('refCodeFormLogin');
			e.setHTTPMethod('POST');
			e.setRefCode();

			}



			var returnRefCodeLogin = function(results) {

			if (results.ERROR ==1) {

			document.getElementById('divRefCode').innerHTML = results.MESSAGE;

			} else if(results.ERROR ==2) {

			} else {
			document.getElementById('divRefCode').innerHTML = results.REFCODE;
			}
			}

			
			

